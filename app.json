{
  "expo": {
    "scheme": "sims",
    "name": "SIMS",
    "slug": "SIMS",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",

    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "newArchEnabled": true,

    "ios": {
      "supportsTablet": true
    },
    "android": {

      "edgeToEdgeEnabled": true
    },
    "web": {

    },
    "plugins": [
      "expo-router"
    ]
  }
}
